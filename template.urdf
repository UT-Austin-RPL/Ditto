			<origin rpy="0.0 0.0 0.0" xyz="joint_position_r"/>
			<geometry>
				<mesh filename="mobile.obj"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0.0 0.0 0.0" xyz="joint_position_r"/>
