			<geometry>
				<mesh filename="static.obj"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="static.obj"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
